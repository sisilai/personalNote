sql记录:select sr.id,sr.bill_id,sr.type,sr.from_storehouse,sr.to_storehouse,sr.date_time,sr.employee_id,sr.comment,
sr.status,sr.member_id,sr.member_name,sri.storage_item_id,abs(sri.amount) amount,sri.employee_id employee_ids
from tb_storage_record sr,tb_storage_record_items sri where sr.id = sri.storage_record_id and sr.master_id = sri.master_id and
sr.master_id = :masterId and sri.storage_item_id = :storageItemId and sr.status != :status and sr.date_time between :startTime and :endTime
and (sr.from_storehouse = :storehouseId or sr.to_storehouse = :storehouseId) and
sr.enterprise_id in (:columnName0,:columnName1,:columnName2,:columnName3,:columnName4,:columnName5) order by sr.date_time desc
